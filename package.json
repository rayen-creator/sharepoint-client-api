{
  "name": "sharepoint-client-api",
  "version": "1.0.7",
  "description": "A fluent TypeScript/JavaScript wrapper for interacting with SharePoint site and admin APIs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "release": "npm run build && npm version patch && npm publish --access public",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "sharepoint",
    "typescript",
    "javascript",
    "microsoft",
    "api-client",
    "api",
    "wrapper",
    "sdk",
    "odata"
  ],
  "author": "Rayen Oueslati",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rayen-creator/sharepoint-sharepoint-client-api"
  },
  "homepage": "https://github.com/rayen-creator/sharepoint-sharepoint-client-api",
  "bugs": {
    "url": "https://github.com/rayen-creator/sharepoint-sharepoint-client-api/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "qs": "^6.14.0"
  }
}
